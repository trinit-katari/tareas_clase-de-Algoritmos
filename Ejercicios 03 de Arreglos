# Inicializar una lista vacía para guardar los múltiplos
multiplos = []

# Generar 15 múltiplos de 16 empezando desde 160
for i in range(15):
    multiplo = 160 + (i * 16)
    multiplos.append(multiplo)

# Imprimir los múltiplos guardados
print("Los 15 múltiplos de 16 desde 160 son:")
for numero in multiplos:
    print(numero)
