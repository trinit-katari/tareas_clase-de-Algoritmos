#include <stdio.h>

int main() {
    int n, i, multiplicador;

    printf("¿Cuántos números desea ingresar? ");
    scanf("%d", &n);

    int arreglo[n];

    for(i = 0; i < n; i++) {
        printf("Número %d: ", i+1);
        scanf("%d", &arreglo[i]);
    }

    printf("Ingrese el número multiplicador: ");
    scanf("%d", &multiplicador);

    printf("Resultados:\n");
    for(i = 0; i < n; i++) {
        arreglo[i] *= multiplicador;
        printf("%d ", arreglo[i]);
    }

    return 0;
}
