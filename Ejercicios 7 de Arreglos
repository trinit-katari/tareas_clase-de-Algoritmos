#include <stdio.h>

int main() {
    int A[10], B[10], i;

    printf("Ingrese 10 números:\n");
    for(i = 0; i < 10; i++) {
        printf("A[%d]: ", i);
        scanf("%d", &A[i]);
    }

    for(i = 0; i < 10; i++) {
        B[i] = A[9 - i];
    }

    printf("Arreglo A: ");
    for(i = 0; i < 10; i++) printf("%d ", A[i]);

    printf("\nArreglo B (al revés): ");
    for(i = 0; i < 10; i++) printf("%d ", B[i]);

    return 0;
}
