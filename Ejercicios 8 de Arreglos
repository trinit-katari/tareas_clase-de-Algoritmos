#include <stdio.h>

int main() {
    int A[10], i, j, temp;

    printf("Ingrese 10 n√∫meros:\n");
    for(i = 0; i < 10; i++) scanf("%d", &A[i]);

    // Ordenamiento burbuja
    for(i = 0; i < 9; i++) {
        for(j = i + 1; j < 10; j++) {
            if(A[i] > A[j]) {
                temp = A[i];
                A[i] = A[j];
                A[j] = temp;
            }
        }
    }

    printf("Ordenados de menor a mayor: ");
    for(i = 0; i < 10; i++) printf("%d ", A[i]);

    return 0;
}
