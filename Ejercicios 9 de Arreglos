#include <stdio.h>

int main() {
    int A[10], PAR[10], IMPAR[10];
    int i, p = 0, imp = 0;

    printf("Ingrese 10 n√∫meros:\n");
    for(i = 0; i < 10; i++) {
        scanf("%d", &A[i]);
        if(A[i] % 2 == 0) PAR[p++] = A[i];
        else IMPAR[imp++] = A[i];
    }

    printf("A: ");
    for(i = 0; i < 10; i++) printf("%d ", A[i]);

    printf("\nPAR: ");
    for(i = 0; i < p; i++) printf("%d ", PAR[i]);

    printf("\nIMPAR: ");
    for(i = 0; i < imp; i++) printf("%d ", IMPAR[i]);

    return 0;
}
